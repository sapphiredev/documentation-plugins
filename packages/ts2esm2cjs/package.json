{
	"name": "@sapphire/docusaurus-plugin-ts2esm2cjs",
	"version": "1.0.1",
	"description": "Docusaurus Remark plugin for converting TypeScript code to ESM and CJS code",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup && tsc -b src",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@sapphire/prettier-config": "^1.2.7",
		"esm-to-cjs": "^1.2.0",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/ts2esm2cjs"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"docusaurus",
		"docusaurus-plugin",
		"@sapphire",
		"typescript",
		"ts",
		"esm",
		"cjs",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	}
}
