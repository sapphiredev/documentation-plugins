{
	"name": "@sapphire/docusaurus-plugin-ts2esm2cjs",
	"version": "2.0.1",
	"description": "Docusaurus Remark plugin for converting TypeScript code to ESM and CJS code",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"test": "vitest run"
	},
	"dependencies": {
		"@prettier/sync": "^0.3.0",
		"esm-to-cjs": "^1.2.1",
		"mdast-util-mdx": "^3.0.0",
		"typescript": "^5.3.3",
		"unified": "^11.0.4",
		"unist-util-visit": "^5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/ts2esm2cjs"
	},
	"files": [
		"dist/",
		"!dist/.tsbuildinfo"
	],
	"engines": {
		"node": ">=18.0"
	},
	"keywords": [
		"docusaurus",
		"docusaurus-plugin",
		"@sapphire",
		"typescript",
		"ts",
		"esm",
		"cjs",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.2.3",
		"@sapphire/framework": "^5.0.4",
		"tsup": "^8.0.1",
		"vitest": "^1.1.0"
	}
}
