{
	"name": "@sapphire/docusaurus-plugin-ts2esm2cjs",
	"version": "1.0.3",
	"description": "Docusaurus Remark plugin for converting TypeScript code to ESM and CJS code",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsc -b src",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@sapphire/prettier-config": "^1.4.3",
		"esm-to-cjs": "^1.2.1",
		"prettier": "^2.6.2",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4",
		"unist-util-visit": "^2.0.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/ts2esm2cjs"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.d*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"docusaurus",
		"docusaurus-plugin",
		"@sapphire",
		"typescript",
		"ts",
		"esm",
		"cjs",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"typescript": "^4.6.4"
	}
}
