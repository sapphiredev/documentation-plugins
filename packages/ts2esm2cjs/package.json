{
	"name": "@sapphire/docusaurus-plugin-ts2esm2cjs",
	"version": "1.1.3",
	"description": "Docusaurus Remark plugin for converting TypeScript code to ESM and CJS code",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"test": "vitest run"
	},
	"dependencies": {
		"@sapphire/prettier-config": "^1.4.5",
		"esm-to-cjs": "^1.2.1",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3",
		"unist-util-visit": "^2.0.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/ts2esm2cjs"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"docusaurus",
		"docusaurus-plugin",
		"@sapphire",
		"typescript",
		"ts",
		"esm",
		"cjs",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.0.1",
		"@sapphire/framework": "^4.4.4",
		"tsup": "^6.7.0",
		"vitest": "^0.31.4"
	}
}
