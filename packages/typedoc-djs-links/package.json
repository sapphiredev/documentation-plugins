{
	"name": "typedoc-plugin-djs-links",
	"version": "1.1.0",
	"description": "Adds support for linking to discord.js.org for DiscordJS types to TypeDoc",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup && tsc -b src",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/typedoc-djs-links"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"@sapphire",
		"discord.js",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"tsup": "^6.1.2",
		"typescript": "^4.7.4"
	}
}
