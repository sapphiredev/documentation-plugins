{
	"name": "typedoc-plugin-djs-links",
	"version": "2.2.1",
	"description": "Adds support for linking to discord.js.org for DiscordJS types to TypeDoc",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/cjs/index.cjs",
	"module": "dist/esm/index.mjs",
	"types": "dist/cjs/index.d.cts",
	"exports": {
		"import": {
			"types": "./dist/esm/index.d.mts",
			"default": "./dist/esm/index.mjs"
		},
		"require": {
			"types": "./dist/cjs/index.d.cts",
			"default": "./dist/cjs/index.cjs"
		}
	},
	"sideEffects": false,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsup && yarn build:rename-cjs-index",
		"build:rename-cjs-index": "node ../../scripts/rename-cjs-index.mjs",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"test": "vitest run"
	},
	"peerDependencies": {
		"typedoc": ">= 0.23.14 || 0.24.x || 0.25.x"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/documentation-plugins.git",
		"directory": "packages/typedoc-djs-links"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"@sapphire",
		"discord.js",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/documentation-plugins/issues"
	},
	"homepage": "https://www.sapphirejs.dev",
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.2.3",
		"discord.js": "^14.14.1",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.2.2"
	}
}
